Include config.d/*

Host github.com
  User git
  HostName github.com
  IdentityFile ~/.ssh/github_id_ed25519

Host *
  ForwardAgent yes
  AddKeysToAgent yes
  Compression no
  ServerAliveInterval 0
  ServerAliveCountMax 3
  HashKnownHosts no
  UserKnownHostsFile ~/.ssh/known_hosts
  ControlMaster no
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist no